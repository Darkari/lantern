{
  "name": "pylantern",
  "version": "0.1.1",
  "description": "A JupyterLab extension.",
  "author": "Tim Paine",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepublish": "npm run build",
    "test": "jest --coverage --collectCoverageFrom=ts/**/*.{js,jsx}"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.css"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.2.0",
    "@jupyterlab/csvviewer": "^0.19.0",
    "@jupyterlab/application": "^0.19.0",
    "@jupyterlab/apputils": "^0.19.0",
    "@jupyterlab/coreutils": "^2.2.0",
    "@jupyterlab/docregistry": "^0.19.0",
    "@jupyterlab/mainmenu": "^0.8.0",
    "@jupyterlab/notebook": "^0.19.0",
    "@jupyterlab/rendermime": "^0.19.0",
    "@phosphor/disposable": "^1.1.2"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.44",
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6"
  }
}
